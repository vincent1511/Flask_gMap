<script>
    seajs.use(["areaBoxNew","cookie"],function(){
        $("#areaSelect").areabox({
            callback:function(obj){
                $("#areaSelect").find(".areaTxt").html(obj.text());

                // 首頁後台統計
                if (/www(\.(dev|debug))?\.591\.com\.tw\/home$/.test(location.href)) {
                    $.post('//' + location.hostname + '/home/data/regionClick?cr=' + $.cookie('urlJumpIp') + '&r=' + obj.data('id'))
                }

                // 設置新的cookie
                $.cookie('urlJumpIp',obj.attr("data-id"),{expires: 30,path: '/',domain:'591.com.tw'});
                // 去掉hash
                window.location.hash = ''

                if (/www(\.(dev|debug))?\.591\.com\.tw/g.test(location.href)) {
                    // 鏈接參數重置&頁面刷新
                    window.location.href = window.location.origin + '?regionid=' + obj.attr("data-id")
                } else {
                    // 頁面刷新
                    window.location.reload();
                }
            }
        });

        // 導航子菜單顯示
        $(".nav-list li").bind({
            "mouseover" : function(){
                $(this).addClass("active").find(".sub-nav").show();
            },
            "mouseout" : function(){
                $(this).removeClass("active").find(".sub-nav").hide();
            }
        })

        // 狀態區域下拉
        $(".login-status .dropMenu").bind({
            "mouseover" : function(){
                $(this).addClass("active").find(".dropbox").show();
            },
            "mouseout" : function(){
                $(this).removeClass("active").find(".dropbox").hide();
            }
        })

        // 獲取登錄提示展示次數
        var _val = $.cookie("loginNoticeNumber") ? $.cookie("loginNoticeNumber") : 0;

        //登錄提示框是否展示，超過三次就不再展示
        if(!$.cookie("loginNoticeStatus") && _val <3){
            $(".login-notice").show();
        }

        // 點擊關閉時同時寫入登錄提示次數cookie和展示cookie
        $(".login-close").on("click",function(){
            $(".login-notice").hide();

            // 控制是否展示
            $.cookie("loginNoticeStatus",1,{expires: 1,path: '/',domain:'591.com.tw'})

            // 打入展示次數
            $.cookie("loginNoticeNumber",parseInt(_val) + 1,{expires: 365,path: '/',domain:'591.com.tw'})

        })
    })
</script>    
    
      

